^"prelude.ceu"
^"int.ceu"
^"pico.ceu"

output pico Pico.Output.Set.Title _("pico-Ce: Across")

var i: _int = _(-25)
loop {
    output pico Pico.Output.Draw.Pixel [i,i]
    input pico Pico.Input.Delay _100
    if _($i == 25) {
        break
    }
    set i = _($i + 1)
}

input pico Pico.Input.Event.Forever _SDL_ANY
